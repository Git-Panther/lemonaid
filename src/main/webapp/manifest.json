{
	"_version": "1.2.0",
	"sap.app": {
		"_version": "1.2.0",
		"id": "com.sap.mentors.lemonaid",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.1worklist",
			"version": "1.36.2"
		},
		"dataSources": {
			"odata": {
				"uri": "/odata.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "/localService/metadata.xml"
				}
			},
			"user": {
				"uri": "/userapi/currentUser",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"_version": "1.2.0",
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "com.sap.mentors.lemonaid.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.36.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.36.0"
				},
				"sap.m": {
					"minVersion": "1.36.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"resourceRoots": {
			"openui5.googlemaps": "./util/openui5_googlemaps/",
			"google.maps": "openui5/googlemaps/loadScripts"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sap.mentors.lemonaid.i18n.i18n"
				}
			},
			"user": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "user",		
				"settings": {
					"defaultBindingMode": "OneWay"
				}
			},
			"": {
				"dataSource": "odata",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"json": true
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.sap.mentors.lemonaid.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide"
			},
			"routes": [{
				"pattern": "",
				"name": "Main",
				"target": "main"
			},{
				"pattern": "Mentors",
				"name": "Mentors",
				"target": "mentors"
			},{
				"pattern": "Mentor/{Id}",
				"name": "Mentor",
				"target": "mentor"
			},{
				"pattern": "Events",
				"name": "Events",
				"target": "events"
			},{
				"pattern": "Content",
				"name": "Content",
				"target": "content"
			},{
				"pattern": "SocialMedia",
				"name": "SocialMedia",
				"target": "socialMedia"
			}],
			"targets": {
				"main": {
					"viewName": "Main",
					"viewLevel": 0
				},
				"mentors": {
					"viewName": "Main",
					"viewLevel": 0
				},
				"mentor": {
					"viewName": "Main",
					"viewLevel": 0
				},
				"events": {
					"viewName": "Main",
					"viewLevel": 0
				},
				"content": {
					"viewName": "Main",
					"viewLevel": 0
				},
				"socialMedia": {
					"viewName": "Main",
					"viewLevel": 0
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}