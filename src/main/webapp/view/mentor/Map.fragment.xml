<core:FragmentDefinition
	xmlns="openui5.googlemaps"
	xmlns:core="sap.ui.core">
    <Map id="idMap" lat="{Latitude}" lng="{Longitude}" width="100%" height="350px" zoom="4">
        <markers>
            <Marker lat="{Latitude}" lng="{Longitude}" info="{mentors>info}" />
        </markers>
    </Map>
</core:FragmentDefinition>